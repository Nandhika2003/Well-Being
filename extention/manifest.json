{
    "manifest_version": 2,
    "name": "Well Being",
    "description": "",
    "short_name": "wellbeing",
    "version": "1.0.0",
    "browser_action": {
      "default_popup": "index.html",
      "default_icon" : "images/icon_128.png"
    },
    "icons": {
      "16": "images/icon_16.png",
      "48": "images/icon_48.png",
      "128": "images/icon_128.png"
    },
    "background": {
      "scripts": [
        "js/background/main.js"
      ]
    },
    "permissions": [
      "storage",
      "notifications",
      "tabs",
      "activeTab",
      "webNavigation",
      "webRequest",
      "nativeMessaging",
      "http://127.0.0.1:5500/src/assets/js/hiii.html"
    ],
    "content_security_policy": "script-src 'self'; object-src 'self'"
  }
  
